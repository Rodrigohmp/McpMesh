{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "McpMeshOptions": {
    "Packages": [
      {
        "Enabled": true,
        "Id": "example-package",
        "Servers": [ "everything-server", "time-server" ]
      }
    ],
    "Servers": [
      {
        "Id": "everything-server",
        "Name": "MCP Everything Server",
        "Type": "stdio",
        "Command": "npx",
        "Args": [ "@modelcontextprotocol/server-everything" ],
        "Environment": {},
        "Enabled": true,
        "TimeoutMs": 30000
      },
      {
        "Id": "time-server",
        "Name": "Time MCP Server",
        "Type": "stdio",
        "Command": "uvx",
        "Args": [ "mcp-server-time" ],
        "Environment": {},
        "Enabled": true,
        "TimeoutMs": 30000
      }
    ],
    "Authentication": {
      "Enabled": false,
      "LoginRequest": {
        "Url": "https://your-auth-server.com/api/auth/login",
        "Method": "POST",
        "BodyTemplate": "{\"username\": \"{username}\",\"password\": \"{password}\"}",
        "ContentType": "application/json"
      }
    },
    "Encryption": {
      "Key": "generate-your-own-32-byte-base64-key",
      "Iv": "generate-your-own-16-byte-base64-iv"
    },
    "BaseUrl": "http://localhost:5293"
  },
  "SessionOptions": {
    "SessionTimeoutMinutes": 30,
    "CleanupIntervalMinutes": 5,
    "MaxSessionsPerPackage": 10
  }
}